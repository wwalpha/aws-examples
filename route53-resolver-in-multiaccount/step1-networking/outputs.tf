# ----------------------------------------------------------------------------------------------
# AWS VPC ID - VPC
# ----------------------------------------------------------------------------------------------
output "vpc_id" {
  value = aws_vpc.this.id
}

# ----------------------------------------------------------------------------------------------
# AWS VPC Subnets - Private Subnets
# ----------------------------------------------------------------------------------------------
output "vpc_public_subnet_ids" {
  value = aws_subnet.public[*].id
}

# ----------------------------------------------------------------------------------------------
# AWS VPC Subnets - Private Subnets
# ----------------------------------------------------------------------------------------------
output "vpc_private_subnet_ids" {
  value = aws_subnet.private[*].id
}
